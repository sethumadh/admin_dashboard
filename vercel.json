{
    "version": 2,
    "builds": [
      {
        "src": "/server/src/index.ts",  
        "use": "@vercel/node"
      },
      {
        "src": "/frontend/dist"    
      }
    ],
  

  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/server/src/index.ts"
      }
    ]
  }